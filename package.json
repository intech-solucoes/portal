{
  "name": "portalprev",
  "version": "1.6.1",
  "private": true,
  "homepage": "http://portal.preves.es.gov.br/",
  "dependencies": {
    "@intechprev/prevsystem-service": "^0.0.9",
    "@intechprev/react-lib": "^0.0.3",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "bootstrap": "^4.0.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cldr": "^4.10.0",
    "cldr-data": "^32.0.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "global": "^4.3.2",
    "globalize": "^1.3.0",
    "globalize-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.1",
    "react-fontawesome": "^1.6.1",
    "react-globalize": "^1.0.0",
    "react-input-mask": "^2.0.2",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-beta.3",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "copy": "node ./node_modules/cpx/bin/ ./node_modules/bootstrap/dist/css/bootstrap.min.css ./public/css",
    "build-css": "sass -t compressed ./src/styles/main.scss ./public/css/main.css",
    "watch-css": "sass -t compressed --watch ./src/styles/main.scss:./public/css/main.css",
    "clean": "node ./node_modules/rimraf/bin.js ./public/css && node ./node_modules/mkdirp/ ./public/css",
    "start": "yarn build && node scripts/start.js",
    "start:preves": "yarn build:config-preves && yarn start",
    "start:preves-rony": "yarn build:config-preves-rony && yarn start",
    "start:regius": "./node_modules/.bin/cpy src/configs/config.regius.dev.json src --rename=config.json && yarn start",
    "build": "yarn clean && yarn copy && yarn build-css",
    "build:config-preves": "./node_modules/.bin/cpy src/configs/config.preves.dev.json src --rename=config.json",
    "build:config-preves-test": "./node_modules/.bin/cpy src/configs/config.preves.test.json src --rename=config.json",
    "build:config-preves-rony": "./node_modules/.bin/cpy src/configs/config.preves.rony.json src --rename=config.json",
    "build:config-preves-prod": "./node_modules/.bin/cpy src/configs/config.preves.prod.json src --rename=config.json",
    "build:config-regius": "./node_modules/.bin/cpy src/configs/config.regius.dev.json src --rename=config.json",
    "build:config-regius-test": "./node_modules/.bin/cpy src/configs/config.regius.dev.json src --rename=config.json",
    "build:prod": "node scripts/build.js",
    "build:preves-test": "yarn build:config-preves-test && yarn build:prod && ./node_modules/.bin/cpy **/* Z:/PortalPrevesTeste --parents --cwd=build",
    "build:preves-prod": "yarn build:config-preves-prod && yarn build:prod && ./node_modules/.bin/cpy **/* Z:/PortalPreves --parents --cwd=build",
    "test": "node scripts/test.js --env=jsdom"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "cpy-cli": "^2.0.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "workspaces": ["../lib/react-lib", "../prevsystem-service"]
}
